{
  "manifest_version": 2,
  "name": "Kelyst",
  "description": "The Real Estate Game Changer",
  "version": "0.1.0",
  "homepage_url": "https://www.kelyst.com",
  "applications": {
      "gecko": {
         "id": "hello@kelyst.com"
      }
   },
  "browser_action": {
    "default_title": "Kelyst"
  },
  "icons": {
    "16": "icon-small-pink-k.png",
    "48": "icon-medium-pink-k.png",
    "128": "icon-large-pink-k.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "icon-large-pink-k.png",
    "graceful.mp3",
    "check.svg"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.kelyst.com/*",
      "http://development.kelyst.com:3000/*"
    ]
  }
}
